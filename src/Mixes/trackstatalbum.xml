<?xml version="1.0" encoding="utf-8"?>
<custombrowse>
	<requireplugins>TrackStat</requireplugins>
	<mix>
		<mixname>TrackStat</mixname>
		<mixcategory>album</mixcategory>
		<mixtype>mode</mixtype>
		<mixchecktype>function</mixchecktype>
		<mixcheckdata>Plugins::TrackStat::Plugin::mixable</mixcheckdata>
		<mixdata>PLUGIN.TrackStat::Plugin|album={itemid}|statistictype=album|flatlist=1</mixdata>
		<mixurl>plugins/TrackStat/all.html?&amp;album={itemid}&amp;flatlist=1</mixurl>
		<mixurlcontext>#songlist</mixurlcontext>
		<miximage>html/images/trackstat_mixbutton.gif</miximage>
	</mix>
</custombrowse>
