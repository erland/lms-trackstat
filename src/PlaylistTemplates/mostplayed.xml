<?xml version="1.0" encoding="utf-8"?>
<trackstat>
	<database>mysql</database>
	<template>
		<name>TrackStat Most played songs</name>
		<description>Random playlist with only most played songs</description>
		<parameter type="text" id="playlistname" name="Playlist name" value="TrackStat Most Played Songs"/>
		<parameter type="text" id="playlistgroups" name="Groups" value=""/>
		<parameter type="text" id="nooftracks" name="Use this number of tracks" value="20"/>
		<parameter type="checkbox" id="notrepeat" name="Do not repeat tracks within same playlist" value="1"/>
	</template>
</trackstat>

